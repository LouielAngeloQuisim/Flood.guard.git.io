#include <WiFiS3.h>
#include <ArduinoHttpClient.h>

const char* ssid = "YOUR_WIFI_SSID";      // Palitan ng WiFi name mo
const char* password = "YOUR_WIFI_PASS";  // Palitan ng WiFi password mo
const char* server = "yourusername.github.io";  // Palitan ng GitHub username mo
const int port = 80;

WiFiClient wifiClient;
HttpClient client = HttpClient(wifiClient, server, port);

void setup() {
    Serial.begin(115200);
    WiFi.begin(ssid, password);

    Serial.print("Kumokonekta sa WiFi...");
    while (WiFi.status() != WL_CONNECTED) {
        delay(1000);
        Serial.print(".");
    }
    Serial.println("\nNakakonekta sa WiFi!");

    // Kumuha ng data mula sa GitHub Pages
    Serial.println("Kinukuha ang impormasyon ng baha...");
    client.get("/yourrepo/data.json");  // Palitan ng pangalan ng repo mo

    int statusCode = client.responseStatusCode();
    String response = client.responseBody();

    Serial.print("HTTP Status Code: ");
    Serial.println(statusCode);
    Serial.print("Response: ");
    Serial.println(response);
}

void loop() {
}
#include <LiquidCrystal_I2C.h>

LiquidCrystal_I2C lcd(0x27, 16, 2);  // Depende sa address ng LCD mo

void setup() {
    lcd.init();
    lcd.backlight();
    lcd.setCursor(0, 0);
    lcd.print("Flood Status:");
}
void loop() {
    lcd.setCursor(0, 1);
    lcd.print("Level: 2.5m");  // Palitan ito ng actual na response data
}
